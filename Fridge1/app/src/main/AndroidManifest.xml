<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kirillrychkov.fridge1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.orm.SugarApp">

        <meta-data android:name="DATABASE" android:value="sugar_example.db"/>
        <meta-data android:name="VERSION" android:value="1"/>
        <meta-data android:name="QUERY_LOG" android:value="true"/>
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="com.example"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Vegetarian" />
        <activity android:name=".Meateater" />
        <activity android:name=".Fasting" />
        <activity android:name=".Setting" />
        <activity android:name=".UserSetting" />
        <activity android:name=".Products" />
        <activity android:name=".Breakfast" />
        <activity android:name=".Dinner" />
        <activity android:name=".Lunch" />
    </application>

</manifest>

